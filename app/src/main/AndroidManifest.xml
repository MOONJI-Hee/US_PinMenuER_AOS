<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wooriyo.pinmenuer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28"
        tools:ignore="ScopedStorage" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PinMenuER"
        tools:targetApi="31">
        <activity
            android:name=".ItemDetailHostActivity"
            android:exported="false"
            android:label="@string/title_activity_item_detail_host"
            android:resizeableActivity="true"
            tools:targetApi="24" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_appKey" />

        <activity
            android:name=".call.CallSetActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".call.CallListActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".order.OrderListActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".menu.MenuSetActivity"
            android:exported="false" />
        <activity
            android:name=".common.MapActivity"
            android:exported="false" />
        <activity
            android:name=".menu.CategorySetActivity"
            android:exported="false" />
        <activity
            android:name=".member.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".member.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".member.MemberSetActivity"
            android:exported="false" />
        <activity
            android:name=".store.StoreListActivity"
            android:exported="false" />
        <activity
            android:name=".store.StoreMenuActivity"
            android:exported="false" />
        <activity
            android:name=".store.StoreSetActivity"
            android:exported="false" />
        <activity
            android:name=".store.StoreSetDetailActivity"
            android:exported="false" />
        <activity
            android:name=".store.StoreSetTimeActivity"
            android:exported="false" />
        <activity
            android:name=".store.StoreSetImgActivity"
            android:exported="false" />
        <activity
            android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>